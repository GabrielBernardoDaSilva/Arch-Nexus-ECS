{
  "name": "arch-nexus-ecs",
  "version": "0.5.0",
  "license": "MIT",
  "description": "Arch Nexus Entity Component System",
  "type": "module",
  "main": "index.d.ts",
  "module": "index.d.ts",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./index.d.ts",
      "require": "./index.d.ts"
    },
    "./src*": "./src/*"
  },
  "keywords": [
    "ecs",
    "game engine",
    "entity component system",
    "data driven design",
    "data management"
  ],
  "author": {
    "name": ""
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "decl": "tsc --declaration --emitDeclarationOnly"
  },
  "devDependencies": {
    "@types/node": "^14.14.7",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.21.0",
    "typescript": "^4.5.2"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ]
  },
  "dependencies": {
    "reflect-metadata": "^0.2.1"
  }
}
